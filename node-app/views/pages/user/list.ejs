<!-- views/pages/user/list.ejs -->

<!DOCTYPE html>
<html lang="en">
<head>
    <% include ../../partials/head %>
</head>
<body class="container">

    <header>
        <% include ../../partials/header %>
    </header>

    <main>
        <div class="jumbotron">
            <p>User Page</p>
            <p><%= tagline %></p>
            <ul>
                <% users.forEach(function(user) { %>
                    <li><%= user.name %> - <%= user.drunkness %></li>
                <% }); %>
            </ul>
        </div>
    </main>

    <footer>
        <% include ../../partials/footer %>
    </footer>
    
</body>
</html>
